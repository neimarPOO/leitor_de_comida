{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "."
      }
    }
  ],
  "routes": [
    {
      "src": "/analyze",
      "dest": "server.py"
    },
    {
      "src": "/(.*)",
      "dest": "index.html"
    }
  ]
}